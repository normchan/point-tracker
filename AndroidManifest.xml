<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="normchan.pointTracker"
      android:versionName="1.3" android:versionCode="4">
    <uses-sdk android:minSdkVersion="8" />

    <application android:label="@string/app_name" android:icon="@drawable/reportcard">
        <activity android:name=".PointTracker"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="PointUpdater"></activity>
        <activity android:name="DetailReport"></activity>
        <activity android:name="HistoryReport">
        </activity>
        <activity android:name="ChildSelector"></activity>

    </application>
</manifest>